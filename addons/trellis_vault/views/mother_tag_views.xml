<odoo>
  <record id="view_trellis_vault_mother_tag_tree" model="ir.ui.view">
    <field name="name">trellis.vault.mother.tag.tree</field>
    <field name="model">trellis.vault.mother.tag</field>
    <field name="arch" type="xml">
      <tree>
        <field name="name"/>
        <field name="strain_id"/>
        <field name="group_no"/>
        <field name="active"/>
      </tree>
    </field>
  </record>

  <record id="view_trellis_vault_mother_tag_form" model="ir.ui.view">
    <field name="name">trellis.vault.mother.tag.form</field>
    <field name="model">trellis.vault.mother.tag</field>
    <field name="arch" type="xml">
      <form>
        <sheet>
          <group>
            <field name="name"/>
            <field name="strain_id"/>
            <field name="group_no"/>
            <field name="active"/>
          </group>
        </sheet>
      </form>
    </field>
  </record>

  <record id="action_trellis_vault_mother_tag" model="ir.actions.act_window">
    <field name="name">Mother Tags</field>
    <field name="res_model">trellis.vault.mother.tag</field>
    <field name="view_mode">tree,form</field>
  </record>
</odoo>
